<h1>Log in</h1>
 <div class="container">
<%= form_tag sessions_path do %>
    <p>
      Email/Username<br>
      <%= text_field_tag :email_or_username, params[:email_or_username] %>
    </p>
    <p>
      <%= label_tag :password %>
      <%= password_field_tag :password %>
    </p>
    <p class="button"><%= submit_tag "Login" %></p>
<% end %>
     </div>